[goto-school]
exe = echo 'go to school'
dep[] = demo:get-a-car

[get-a-car]
ref = demo:buy-a-car
dep[] = demo:settle-in

[settle-in]
exe = echo 'settle in'

[buy-a-car]
exe = echo 'bought a car!'
dep[] = demo:goto-dealer

[goto-dealer]
ref = demo:car-deal
dep[] = demo:get-money

[get-money]
spawn = direct
#exe = ls /root
exe = echo 'get enough money!'

[car-deal]
exe = echo 'OKay, I will buy it!'
dep[] = demo:dealer-price

[dealer-price]
exe = echo 'Yes, it is 1800$!'
dep[] = demo:say-hello-to-dealer

[say-hello-to-dealer]
exe = echo 'Hi, do you have 2015 Toyota?'
